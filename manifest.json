{
   "action": {
      "default_icon": {
         "192": "icons/128.png"
      },
      "default_popup": "/index.html",
      "default_title": "ReactAI"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "content-script.js" ],
      "css": [ "style.css" ],
      "matches": [ "https://twitter.com/*","https://www.linkedin.com/*"],
      "run_at": "document_start"
   } ],
   "description": "Extension for auto-replying using Chat-GPT",
   "host_permissions": [ "http://localhost:5173/","https://react-ai-demo.netlify.app/" ],
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "48": "icons/48.png"
   },
   "manifest_version": 3,
   "name": "Reacti AI",
   "permissions": [   "cookies" ],
   "version": "1.0.7"
}
